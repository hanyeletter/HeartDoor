  dialogueInfos:
  - characterName: "\u5BB6\u957FA"
    characterSprite: {fileID: 0}
    dialogueContent: "\u8981\u6211\u8BF4\uFF0C\u5B66\u6821\u641E\u8FD9\u79CD\u4E1C\u897F\u65E0\u975E\u5C31\u662F\u63A8\u5378\u8D23\u4EFB\u3002"
  - characterName: "\u5BB6\u957FA"
    characterSprite: {fileID: 0}
    dialogueContent: "\u628A\u6559\u80B2\u5B69\u5B50\u8FD9\u4EF6\u4E8B\u513F\u518D\u7529\u56DE\u7ED9\u6211\u4EEC\u505A\u7236\u6BCD\u7684\uFF0C\u90A3\u6211\u82B1\u94B1\u4F9B\u5B69\u5B50\u8BFB\u4E66\u662F\u4E3A\u4E86\u5565\uFF1F"
  - characterName: "\u5BB6\u957FB"
    characterSprite: {fileID: 0}
    dialogueContent: "\u6211\u89C9\u7740\u4ED8\u8001\u5E08\u8BB2\u7684\u90FD\u7279\u522B\u6709\u9053\u7406\uFF0C\u6211\u521A\u521A\u90FD\u505A\u4E86\u7B14\u8BB0\uFF0C\u4E00\u6B65\u6B65\u8BE5\u600E\u4E48\u505A\u6211\u90FD\u5199\u597D\u4E86\u3002"
  - characterName: "\u5BB6\u957FC"
    characterSprite: {fileID: 0}
    dialogueContent: "\u4F60\u4EEC\u554A\uFF0C\u90FD\u592A\u5728\u610F\u8FD9\u4EF6\u4E8B\u60C5\u4E86\u3002"
  - characterName: "\u5BB6\u957FC"
    characterSprite: {fileID: 0}
    dialogueContent: "\u54B1\u4EEC\u5E74\u8F7B\u7684\u65F6\u5019\u4E0D\u4E5F\u559C\u6B22\u80E1\u601D\u4E71\u60F3\u5417\uFF0C\u73B0\u5728\u5462\uFF1F"
  - characterName: "\u5BB6\u957FC"
    characterSprite: {fileID: 0}
    dialogueContent: "\u8BE5\u8FC7\u53BB\u7684\u603B\u4F1A\u8FC7\u53BB\uFF0C\u4F55\u5FC5\u8BA9\u5B69\u5B50\u82B1\u529F\u592B\u5728\u8FD9\u79CD\u4E8B\u60C5\u4E0A\uFF1F\u73B0\u5728\u90FD\u9AD8\u4E8C\u4E86\uFF0C\u5B66\u4E60\u624D\u662F\u7B2C\u4E00\uFF01"
