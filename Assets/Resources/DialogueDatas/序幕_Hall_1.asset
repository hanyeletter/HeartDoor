  dialogueInfos:
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u5404\u4F4D\u5BB6\u957F\u597D\uFF0C\u5F88\u9AD8\u5174\u89C1\u5230\u4F60\u4EEC\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u8FD9\u6B21\u8BB2\u5EA7\u7684\u76EE\u7684\uFF0C\u4E0D\u662F\u7ED9\u5927\u5BB6\u4E0A\u8BFE\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u6211\u4E5F\u4E3A\u4EBA\u7236\u6BCD\uFF0C\u53EA\u662F\u5E0C\u671B\u4E0E\u5927\u5BB6\u4EA4\u6D41\u7ECF\u9A8C\uFF0C\u628A\u6211\u6240\u4E86\u89E3\u7684\u3001\u638C\u63E1\u7684\uFF0C\u5206\u4EAB\u7ED9\u5404\u4F4D\u5BB6\u957F\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u5728\u6B63\u5F0F\u5F00\u59CB\u524D\uFF0C\u6211\u7ED9\u5927\u5BB6\u8BB2\u4E00\u4E2A\u6545\u4E8B\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u57282018\u5E74\uFF0C21\u4E16\u7EAA\u6559\u80B2\u7814\u7A76\u9662\u9881\u5E03\u4E86\u300A\u6559\u80B2\u84DD\u76AE\u4E66\uFF1A\u4E2D\u56FD\u6559\u80B2\u53D1\u5C55\u62A5\u544A\u300B\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u91CC\u9762\u63D0\u5230\u4E00\u4E2A\u5BF93000\u540D\u4E2D\u5B66\u751F\u7684\u95EE\u5377\u8C03\u67E5\uFF0C\u9898\u76EE\u662F\u201C\u4F60\u5BF9\u5F85\u7236\u6BCD\u7684\u6001\u5EA6\u201D\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u7ED3\u679C\u4EE4\u4EBA\u610F\u5916\uFF0C\u4F46\u4F3C\u4E4E\u53C8\u7B26\u5408\u5927\u591A\u6570\u5BB6\u5EAD\u7684\u72B6\u51B5\uFF1A"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u670956.28%\u7684\u5B69\u5B50\u9009\u62E9\u4E86\u201C\u6781\u5EA6\u53CD\u611F\u6216\u75DB\u6068\u7236\u6BCD\u201D\uFF0C\u53EA\u67094.75%\u7684\u5B69\u5B50\uFF0C\u8868\u793A\u201C\u559C\u6B22\u81EA\u5DF1\u7684\u7236\u6BCD\u201D\u3002"
  - characterName: "\u4ED8\u4F69"
    dialogueContent: "\u6709\u6CA1\u6709\u7236\u6BCD\u60F3\u8981\u6765\u56DE\u7B54\u4E00\u4E0B\uFF0C\u5982\u679C\u4F60\u7684\u5B69\u5B50\u6765\u56DE\u7B54\u8FD9\u4E2A\u95EE\u9898\uFF0C\u4F60\u89C9\u5F97\u4ED6\u4F1A\u9009\u62E9\u4EC0\u4E48\u5462\uFF1F"
